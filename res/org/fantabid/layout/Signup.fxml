<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="loginPanel" alignment="TOP_CENTER" minHeight="480.0" minWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" text="FANTABID" textAlignment="CENTER" VBox.vgrow="NEVER">
         <font>
            <Font name="System Bold Italic" size="48.0" />
         </font>
      </Label>
      <GridPane VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints halignment="LEFT" hgrow="NEVER" minWidth="150.0" />
          <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" text="Username" textAlignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <TextField fx:id="usernameField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="5" GridPane.valignment="CENTER" />
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" text="Password" GridPane.rowIndex="7" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="7" GridPane.valignment="CENTER" />
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" text="Confirm" GridPane.rowIndex="9">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" text="Nome" GridPane.rowIndex="1">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" text="Cognome" GridPane.rowIndex="3">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <TextField fx:id="nameField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="surnameField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <PasswordField fx:id="confirmField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="9" />
         </children>
      </GridPane>
      <HBox VBox.vgrow="NEVER">
         <children>
            <Button fx:id="signupButton" alignment="CENTER" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Sign Up">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Button fx:id="loginButton" alignment="CENTER" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Log In">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
